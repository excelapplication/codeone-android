<?xml version="1.0" encoding="UTF-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:width="50dp" android:height="40dp" android:left="5dp">
        <shape>
            <solid android:color="@android:color/holo_green_dark"/>
        </shape>
    </item>
    <item android:width="50dp" android:height="40dp" android:left="50dp">
        <shape>
            <solid android:color="@android:color/holo_green_light"/>
        </shape>
    </item>
    <item android:width="50dp" android:height="40dp" android:left="100dp">
        <shape>
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item
        android:left="50dp"
        android:width="28.28dp" android:height="28.28dp">
        <rotate android:fromDegrees="45"
            android:pivotX="0"
            android:pivotY="0">
            <shape>
                <solid android:color="@android:color/holo_green_dark"/>
            </shape>
        </rotate>
    </item>
    <item
        android:left="100dp"
        android:width="28.28dp" android:height="28.28dp">
        <rotate android:fromDegrees="45"
            android:pivotX="0"
            android:pivotY="0">
            <shape>
                <solid android:color="@android:color/holo_green_light"/>
            </shape>
        </rotate>
    </item>

</layer-list>
